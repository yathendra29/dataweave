	<flow name="Crypto_module_hashWith" doc:id="32d3e007-4d5a-4986-a6c5-dc6a6edd4063" >
		<http:listener doc:name="Listener" doc:id="3af99cc7-a310-41ea-80a0-3973da2813bd" config-ref="HTTP_Listener_config" path="/crypto4"/>
		<ee:transform doc:name="Transform Message" doc:id="85b8edc2-3966-48d5-a3d0-eea6d21c5980" >
			<ee:message >
				<ee:set-payload ><![CDATA[%dw 2.0
output application/json
---
"Hi,This is yathendra"]]></ee:set-payload>
			</ee:message>
		</ee:transform>
		<ee:transform doc:name="Transform Message" doc:id="a5d5a771-984d-4d19-ab7f-7d7af5ecd72b" >
			<ee:message >
				<ee:set-payload ><![CDATA[%dw 2.0
import dw::Crypto
output application/json
---
{
 "MD2":Crypto::hashWith(payload as Binary,"MD2"),
 "MD5":Crypto::hashWith(payload as Binary,"MD5"),
 "SHA-1":Crypto::hashWith(payload as Binary,"SHA-1"),
 "SHA-1":Crypto::hashWith(payload as Binary,"SHA-256"),
 "SHA-1":Crypto::hashWith(payload as Binary,"SHA-384"),
 "SHA-1":Crypto::hashWith(payload as Binary,"SHA-512")
}]]></ee:set-payload>
			</ee:message>
		</ee:transform>
