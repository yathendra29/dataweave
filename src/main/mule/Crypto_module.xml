	<flow name="Crypto_module_HMACBinary" doc:id="0b36bbd4-be57-4511-85b0-c7008c1caabd" >
		<http:listener doc:name="Listener" doc:id="1f887499-f22d-419c-b95d-2f3cf5b1c3ea" config-ref="HTTP_Listener_config" path="/crypto"/>
		<ee:transform doc:name="Transform Message" doc:id="caf43111-3280-426f-93b0-680840345a08" >
			<ee:message >
				<ee:set-payload ><![CDATA[%dw 2.0
output application/json
---
"Hi,This is yathendra"]]></ee:set-payload>
			</ee:message>
		</ee:transform>
		<ee:transform doc:name="Transform Message" doc:id="cbb8baf3-4520-418b-a641-4723c44710f4" >
			<ee:message >
				<ee:set-payload ><![CDATA[%dw 2.0
import dw::Crypto
output application/json
---
 Crypto::HMACBinary("msg" as Binary, payload as Binary)  //key,content
]]></ee:set-payload>
			</ee:message>
		</ee:transform>
	</flow>
	<flow name="Crypto_module_HMACWith" doc:id="ec0b1cb9-9c87-456f-8bb0-1de8e14bad44" >
		<http:listener doc:name="Listener" doc:id="bdb74622-a920-44d1-8a95-a57c7430f6e0" config-ref="HTTP_Listener_config" path="/crypto1"/>
		<ee:transform doc:name="Transform Message" doc:id="c25d9018-78dd-4b7e-967e-7aab7425d529" >
			<ee:message >
				<ee:set-payload ><![CDATA[%dw 2.0
output application/json
---
"Hi,This is yathendra"]]></ee:set-payload>
			</ee:message>
		</ee:transform>
		<ee:transform doc:name="Transform Message" doc:id="7c282261-7956-4019-b3ff-5724bd4794dc" >
			<ee:message >
				<ee:set-payload ><![CDATA[%dw 2.0
import dw::Crypto
output application/json
---
{
 "HmacSHA256":Crypto::HMACWith("msg" as Binary, payload as Binary,"HmacSHA256"),
  "HmacSHA512":Crypto::HMACWith("msg" as Binary, payload as Binary,"HmacSHA512")
}

// "HmacSHA256" smalllength 
// "HmacSHA512" big length]]></ee:set-payload>
			</ee:message>
		</ee:transform>
	</flow>
